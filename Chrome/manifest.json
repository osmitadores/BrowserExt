{
  "manifest_version": 2,

  "name": "Mitatizador",
  "description": "Mitifique a web!",
  "version": "0.1",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["mythMain.js"],
      "css":["teste.css"]
    }
  ]
}
